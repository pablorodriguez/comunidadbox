- javascript 'accounts'
- resource.address = Address.new if resource.address.nil?
- resource.cars.build if resource.cars.empty?

%h2 Nuevo Usuario
= form_for resource, :as => resource_name,:url => registration_path(resource_name) do |f|
  = devise_error_messages!
  %table
    %tbody
      = render 'users/user_row',:f => f
  
  %h2 InformaciÃ³n de su auto
  =f.fields_for :cars do |c|
    = render 'users/car_fields',:f =>c
  
  %h2 DirecciÃ³n
  =f.fields_for :address do |a|
    = render 'users/user_address_fields', :f =>a
  %p= f.submit "Grabar"
= render :partial => "devise/shared/links"