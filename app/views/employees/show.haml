- javascript 'accounts'
- stylesheets 'jquery.ui.base.css'
- stylesheets 'jquery.ui.theme.css'
#account
  %ul
    %li
      %a{:href=>"#user"} Usuario
    %li{:id=>"company_li"}
      %a{:href=>"#company"} Prestador de Servicio        
    %li
      %a{:href=>"#address"} Direcciones
  #user
    = render 'user_show'
  #company
    = render '/companies/show'      
  #address
    = render 'address_show'
      
= link_to image_tag('edit.png'),edit_employee_path(@user)      
= link_to_back
