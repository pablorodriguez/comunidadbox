.car.right
  .box_bottom
    %h1=t(".car")
    = f.hidden_field :car_id
  %table
    %tr
      %td=t(".domain")
      %td
        .domain_white
          = link_to car.domain,car
    %tr
      %td{:style=>"width:100px"}=t(".brand")
      %td 
        = car.brand.name       
    %tr
      %td=t(".model")
      %td 
        = car.model.name       
   
