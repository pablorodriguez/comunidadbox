= form_for resource, :url => user_registration_path do |f|
  .box_bottom
    .left
      %h1=t(".new_user")

    .right
      %h1         
        = select_tag(:user_type,options_for_select(User::TYPES.collect{|k,v| [v,k]},:selected =>resource.type),{:class=>'long_selec'})
  .clear
    = render "form", :f => f,:recaptcha => true