<% if @msg != "" %>
    $("#km_msg").html("<%= @msg%>").show();    
    $("#workorder_submit").attr("disabled","disabled");
<%else%>
  $("#km_msg").html("").hide();
  $("#car_km").val("<%=@car.km%>");
  $("#km").html("<%=@car.km%>").effect("highlight",{},3000);
  $("#car_kmAverageMonthly").val("<%=@car.kmAverageMonthly%>");
  $("#kmAverageMonthly").html("<%=@car.kmAverageMonthly%>").effect("highlight",{},3000);
  $("#kmUpdatedAt").html("<%=l @car.kmUpdatedAt.to_date%>").effect("highlight",{},3000);
  $("#wo_save").show();
  $("#km_update").hide();
  $("#workorder_submit").removeAttr('disabled');
<%end%>