$(".data").hide();
if (<%=@company_cars.size%> > 0){  
  $("#cars").html("<%=escape_javascript(render(:partial=>'cars',:locals =>{:cars=>@company_cars}))%>").show();  
}else{
  $("#cars").html("<%=escape_javascript(render(:partial=>'shared/message',:locals =>{:msg =>"No se encontró ningún autómovil."}))%>").show();
}
