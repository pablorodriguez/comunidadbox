- javascript 'jquery.formatCurrency-1.4.0'
- javascript 'jquery.formatCurrency.es-AR'
- javascript 'jquery.services'
#work_order_info
  .cont1.box_bottom
    %h1 
      Ingrese Servicios Realizados:
      = @work_order.company.name if @work_order.company
      = @work_order.company_info if @work_order.company_info
        
  = render 'shared/error_messages', :target => @work_order
  #work_order
    #car_info.barradatos
      = render :partial=>'cars/car_info',:locals =>{:car=>@work_order.car}
  #materials.hide
    .materials
      = render :partial=>'materials'
  #work_order_form.box_top
    = render :partial=>'services_list',:locals =>{:work_order=>@work_order}  
  #comment_modal.hide         
    Por favor ingrese un comentario
    %br
    = text_area_tag :comment,"test", :rows => 3,:cols=>"28"
    .right
      = link_to "Grabar","#",{:class =>:button,:id=>"save_comment"}
