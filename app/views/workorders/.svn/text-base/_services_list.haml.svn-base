-form_for @work_order do |wo_form|
  = wo_form.error_messages
  #services
    = wo_form.hidden_field :car_id
    %table.service_table
      %tr.header_text_lable
        %td{:style=>'width:80%'}
          %label.left 
            Orden de Trabajo [Estado:
            = @work_order.status
            ]
        %td.number{:style=>'width:15%'}
          #total_work_order
            = number_to_currency @work_order.total_price
        %td{:style=>'width:5%'}
    #services_list
      -wo_form.fields_for :services do |s_form|
        = render "service_fields",:f => s_form
    #n_s{:style=>"display:none"}
      = button_to_add_fields  "Nuevo Servicio",wo_form,:services 
    = wo_form.submit "Grabar"
    = link_to_back    
            
