- javascript 'workorder'
- javascript 'rank'
- javascript 'jquery.ba-bbq.min'
- javascript 'highcharts'
:javascript
  $(document).ready(function() {;
    build_price_graph([#{@price_data}],"price_graph","FacturaciÃ³n por Tipo de Servicio");
    build_amt_graph([#{@amt_data}],"amt_graph","Cantidad por Tipo de Servicio");
  })

%all-workorder
  .vertical_menu
    #menu_options.box_bottom
      %ul
        %li
          #service_done.link.menu.selected
            = image_tag "service.png"
            Servicios Realizados        
        %li
          #report_amount.link.menu
            = image_tag "graph.png"
            Reporte Importes
        %li
          #report_quantiy.link.menu
            = image_tag "graph.png"
            Reporte Cantidad
        %li
          #report_detail.link.menu
            = image_tag "report.png"
            Reporte Detalle


    #menu_actions.box_bottom
      = render 'search'

  .contentright_s
    - if @work_orders.size > 0
      - unless current_user.is_employee
        #workorders_c.data
          #sort_by      
            = render 'sort_by'
          #workorders
            = render 'workorders'     
        #price_graph_c.data.hide.chart
          %h1 Importes por Tipo de Servicios
          #price_graph
        #amt_graph_c.data.hide.chart
          %h1 Cantidad por Tipo de Servicios
          #amt_graph
        - if current_user.is_administrator
          #report_data.data.hide                      
            = render 'totals'
      - else  
        #data.overflow
          #sort_by      
            = render 'sort_by'
          #workorders
            = render 'workorders'
    - else
      = render 'shared/message', :msg =>"No hay servicios realizados"
