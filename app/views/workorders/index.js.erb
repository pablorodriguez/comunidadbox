if (<%=@work_orders.size%> > 0){
  $("#workorders").html("<%=escape_javascript(render(:partial=>'workorders'))%>");
}else{
  $("#workorders").html("<%=escape_javascript(render(:partial=>'shared/message',:locals =>{:msg =>"No se encontró ninguna orden de trabajo."}))%>");  
} 
$("#totals").html("<%=escape_javascript(render(:partial=>'totals')) if current_user.is_administrator%>");
if ($("#price_graph").size() >0){ 
  build_price_graph([<%=@price_data%>],"price_graph","Facturación por Tipo de Servicio");
}
if ($("#amt_graph").size() > 0){
  build_amt_graph([<%=@amt_data%>],"amt_graph","Cantidad por Tipo de Servicio");
}
$(".labelify").labelify({ labelledClass: "labelHighlight" });