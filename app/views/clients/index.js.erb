if (<%=@clients.size%> > 0){
  $("#clients").html("<%=escape_javascript(render(:partial=>'clients'))%>");
   showHideContent($("#clients_link"),"#clients");
}else{
  $("#clients").html("<%=escape_javascript(render(:partial=>'shared/message',:locals =>{:msg =>"No se encontró ningún cliente."}))%>");
}
$(".labelify").labelify({ labelledClass: "labelHighlight" });
