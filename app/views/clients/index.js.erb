$(".data").hide();
if (<%=@clients.size%> > 0){
  $("#clients").html("<%=escape_javascript(render(:partial=>'list_clients',:locals => {:clients => @clients}))%>").show();
}else{
  $("#clients").html("<%=escape_javascript(render(:partial=>'shared/message',:locals =>{:msg =>"No se encontró ningún cliente."}))%>").show();
}
