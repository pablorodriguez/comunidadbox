!!!
%html{:xmlns => "http://www.w3.org/1999/xhtml"}
  %head
    = csrf_meta_tag
    %title Comunidad Box
    %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-Type"}
    %script{:type => "text/javascript",:src  => "http://maps.google.com/maps/api/js?sensor=true"}
    -#= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js", "https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.11/jquery-ui.min.js", "jquery.rails.js"
    = javascript_include_tag 'jquery-1.5.1','jquery.ui.1.8.11','jquery.rails'
    = javascript_include_tag 'jquery.ui.datepicker-es','application'
    = javascript_include_tag 'menu'
    = javascript 'analytics'
    = stylesheet_link_tag 'combox'
    = yield(:head)
    %link{:href => "http://fonts.googleapis.com/css?family=Ubuntu", :rel => "stylesheet", :type => "text/css"}
  %body
    #header
      #logo
        #logologo
          %img{:align => "middle", :alt => "logo", :height => "84", :src => "/images/logo.png", :width => "246"}/
          = render "layouts/logininfo"
    =render "layouts/menu"
    #content
      = render "layouts/msg"
      = yield
    = render :partial => "/layouts/footer"