!!!
%html{:xmlns => "http://www.w3.org/1999/xhtml"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %link{:rel=>"shortcut icon",:href=>"/favicon.ico"}/
    
    %title Comunidad Box
    -#= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyD1AHadsBaiDNWmI6nVvlW3YSDj4cjczas&sensor=false"    
    -#= javascript_include_tag "https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"    
    -#= javascript_include_tag "//maps.google.com/maps/api/js?v=3.8&sensor=false&key=&libraries=geometry&language=&hl=&region="
    = stylesheet_link_tag    "application", :media => "all"    
    = javascript_include_tag "application"
    -#= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=true"
    -#= javascript_include_tag "https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"
    = csrf_meta_tag
    -#= javascript_include_tag 'jquery-1.5.1','jquery.ui.1.8.11','jquery.rails'    
    -#= javascript_include_tag 'jquery-ui-timepicker-addon','jquery.ui.datepicker-es','jquery-ui-timepicker-es','application','autocolumn.min','jquery.labelify','s3Slider','jquery.editinplace'
    -#= stylesheet_link_tag 'combox','jquery-ui-1.8.15.custom','user','buttons','s3slider','gmaps4rails' 
    -#= javascript 'analytics'    
    = yield(:head)
    -#%link{:href => "http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800", :rel => "stylesheet", :type => "text/css"}
    
  %body
    #container
      #header
        #logo
          #logologo
            = link_to image_tag("logo_n.png"),root_path            
            = render "layouts/logininfo"   
      #body        
        .box
          = render "layouts/msg"
          = yield
      #footer        
        = render "/layouts/footer"
    -#= render "layouts/mediabar"

