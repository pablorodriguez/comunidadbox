if($('#service_id').val() !=''){  
  $("#service_<%=@service.row%>").html("<%=escape_javascript(render(:partial=>'service', :locals =>{:service =>@service}))%>");  
  $("#service_<%=@service.row%>").css("backgroundColor", 'white');
}else{
  $("#service_table tr:last").after("<%=escape_javascript(render(:partial=>'new_service', :locals =>{:service =>@service}))%>");
  $("#service_<%=@service.row%>").effect('highlight',{},3000);
}
$('#material_total_price').html('0');
$('#total_price').html('0');
$("#add_service").val("Agregar Servicio");
$('#service_total_price').html("$ <%=@workOrder.total_price%>");
$('#service_total_price').effect('highlight',{},3000);
$('#material_service_type_id').enable();
$("#material_service_type_id option[value='']").attr('selected', 'selected');
$("#service_type_task").html("");
$.each($(".material_row"),function(index,value){
	$(value).remove();
})
AjaxLoader.disable("#service_ajax_loader");
