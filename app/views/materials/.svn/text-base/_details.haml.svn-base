%table.service_table
  %tr
    %td.table_header{:colspan=>3}
      Materiales Encontrados
  - for material in @materials
    %tr
      %td
        = check_box :material_service_type,:id,{:id=>material.material_service_type_id}
      %td
        = material.detail
      %td.number    
        = number_to_currency(material.price)
      %td
        %label{:id=>:service_type_id,:style=>"display:none"}
          = material.service_type_id
= button_to_function "Agregar","add_material_service_type()"
= will_paginate @materials
